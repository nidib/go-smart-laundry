{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "no-use-before-define": "off",
      "no-alert": "off",
      "no-console": "off",
      "semi": ["error", "always"],
      "quotes": ["error", "double"],
      "max-len": "off"
    }
}
